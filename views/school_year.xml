<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
        <record id="view_tree_school_year" model="ir.ui.view">
        <field name="name">year.tree.view</field>
        <field name="model">school.year</field>
        <field name="arch" type="xml">
            <tree string="channel">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="view_form_school_years" model="ir.ui.view">
        <field name="name">year.form</field>
        <field name="model">school.year</field>
        <field name="arch" type="xml">
            <form string="class_form">
                <sheet>
                    <group>
                        <group>
                        <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_school_year" model="ir.actions.act_window">
        <field name="name">Year</field>
        <field name="res_model">school.year</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>